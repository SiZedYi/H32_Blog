{
  "tabWidth": 2,
  "semi": false,
  "useTabs": true,
  "printWidth": 100,
  "singleQuote": true,
  "insertPragma": false,
  "bracketSpacing": true,
  "requirePragma": false,
  "trailingComma": "all",
  "htmlWhitespaceSensitivity": "css, html, js",
  "overrides": [{
    "files": ".prettierrc.json",
    "options": {
      "parser": "json"
    }
  }]
}
