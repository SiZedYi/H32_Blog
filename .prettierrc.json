{
  "tabWidth": 2,
  "semi": false,
  "useTabs": true,
  "printWidth": 100,
  "singleQuote": true,
  "proseWrap": "always",
  "insertPragma": false,
  "bracketSpacing": true,
  "requirePragma": false,
  "trailingComma": "all",
  "quoteProps": "as-needed",
  "htmlWhitespaceSensitivity": "css, html, js"
}
